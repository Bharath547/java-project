pipeline{
 agent any
  stages{
   stage('Checkout Repo') {
    steps{
     checkout([$class: CheckOutClass, branches: [[name: BranchName]], browser: [$class: BrowserClass], userRemoteConfigs: [[credentialsId: PrivateKeyID, url: SourceURL]]])
    }
   }
   stage('Clean'){
    steps{
     sh 'rm -rf $ArtifactsPath'
    }
   }
   stage('build'){
    steps{
     dir(ApplicationPath) {
      sh 'pkg --out-path $ArtifactsPath app.js'
     }
    }
   }
  }
 }
